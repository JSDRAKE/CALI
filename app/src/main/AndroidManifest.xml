        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Materias" />
        <activity android:name=".Algoritmica" />
        <activity android:name=".Analisis" />
        <activity android:name=".Logica" />
        <activity android:name=".Horarios" />
        <activity android:name=".Informacion" />
        <activity android:name=".PantallaActualizar" />
        <activity android:name=".BuscarActualizacion"></activity>
    </application>

</manifest>